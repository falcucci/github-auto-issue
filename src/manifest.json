{
  "applications": {
    "gecko": {
      "id": "alex.falcucci@gmail.com",
      "strict_min_version": "102.0"
    }
  },
  "manifest_version": 2,
  "minimum_chrome_version": "105",
  "name": "github-autoissue",
  "version": "0.0.0",
  "content_scripts": [{
    "run_at": "document_start",
    "matches": [
      "https://github.com/*/projects/*"
    ],
    "js": [
      "content.js"
    ],
    "css": [
      "css/window.css",
      "css/options.css",
      "css/content.css"
    ]
  }],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "48": "images/final-state-48.png"
    },
    "default_title": "Github Autoissue",
    "default_popup": "options.html"
  },
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "activeTab",
    "alarms",
    "https://api.github.com/*",
    "https://github.com/*",
    "https://jsbin-user-assets.s3.amazonaws.com/*",
    "https://jsbin-user-assets.s3.amazonaws.com/rafaelcastrocouto/password.ttf"
  ],
  "optional_permissions": [
    "*://*/*"
  ]
}
